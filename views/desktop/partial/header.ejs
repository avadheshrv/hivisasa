<div class="wrap default" id="nav_inner_wrap">
    <div class="inner-wrap bottom_block_with_search">		
            <div class="logo">
                <a href="/"><img src="/img/logo.png"></a>
            </div>
            <div class="dropdown">
                <select name="county" class="dropdown_list county_selector">
                    <option <% if( county !== undefined && county == "" ){ %> selected="selected" <% } %> value="">Select county</option>
                    <option <% if( county !== undefined && county == "national" ){ %> selected="selected" <% } %> value="national">National</option>
                    <option <% if( county !== undefined && county == "garissa" ){ %> selected="selected" <% } %> value="garissa">Garissa</option>
                    <option <% if( county !== undefined && county == "kiambu" ){ %> selected="selected" <% } %> value="kiambu">Kiambu</option>
                    <option <% if( county !== undefined && county == "kibera" ){ %> selected="selected" <% } %> value="kibera">Kibera</option>
                    <option <% if( county !== undefined && county == "kisii" ){ %> selected="selected" <% } %> value="kisii">Kisii</option>
                    <option <% if( county !== undefined && county == "kisumu" ){ %> selected="selected" <% } %> value="kisumu">Kisumu</option>
                    <option <% if( county !== undefined && county == "machakos" ){ %> selected="selected" <% } %> value="machakos">Machakos</option>
                    <option <% if( county !== undefined && county == "mombasa" ){ %> selected="selected" <% } %> value="mombasa">Mombasa</option>
                    <option <% if( county !== undefined && county == "nakuru" ){ %> selected="selected" <% } %> value="nakuru"> Nakuru</option>
                    <option <% if( county !== undefined && county == "nyamira" ){ %> selected="selected" <% } %> value="nyamira">Nyamira</option>
                    <option <% if( county !== undefined && county == "uasin-gishu" ){ %> selected="selected" <% } %> value="uasin-gishu">Uasin-Gishu</option>
                </select>
            </div>
            <div class="btn_search">
                <i class="fa fa-search" aria-hidden="true"  id="btn_search_on_phrase"></i>				
            </div>
            <a href="" class="btn_write_for_us">
                <div class="">
                        write for us!
                </div>
            </a>
            <a href="" class="btn_login">
                <div class="">
                        log in
                </div>
            </a>
    </div>
    <div class="inner-wrap search_tool_pad">
        <form>
            <input class="search_input" placeholder="Search term.."></input>
            <button class="btn_go_search">Search</button>
        </form>			
    </div>
</div>
<div class="wrap news_name_and_date">
    <div class="inner-wrap">
        <% if( county != undefined && county != "" ){ %>
        <div class="news_name">
            <%= county %> <span class="name_news_blue_color">news</span>
        </div>
        <% } %>
        <div class="todays_date">
                Monday, 21 December
        </div>
    </div>
    <div class="clear">
    </div>
</div>
